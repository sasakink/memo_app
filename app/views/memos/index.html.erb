<body>
<header>
<h1>トップページ</h1>
  <div>
    <%= link_to image_tag("tabi_memo.png", class:"header-image")%>
  </div>
</header>
<div class="contents">
<%= link_to 'メモ新規投稿', '/memos/new', class: "post-memo"%>
<% @memos.each do |memo| %>
<div class="contents">
  <div class="memo">
    <div class="memo-date">
      投稿日時：<%= memo.created_at %>
    </div>
    <div class="memo-content">
      <%= memo.content %>
    </div>
  </div>
</div>
<% end %>
</body>
<footer>
</footer>